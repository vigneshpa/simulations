{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": false,
    "baseUrl": ".",
    "moduleResolution": "node",
    "importsNotUsedAsValues": "error",
    "sourceMap": true,
    "declaration": false,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ESNext.Array"
    ]
  },
  "include": [
    "src/**/*.ts",
    "typings/**/*.d.ts"
  ]
}